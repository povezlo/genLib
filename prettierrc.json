{
  "extends": [
    "prettier"
  ],
  "singleQuote": true,
  "trailingComma": "all",
  "semi":  true,
  "printWidth":  120,
  "tabWidth":  2,
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "editor.fontSize": 16,
  "endOfLine":"lf"
}
